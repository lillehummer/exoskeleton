    "webpack-dev-middleware": "*",
    "webpack-hot-middleware": "*",
    "buble-loader": "*",
    "vue-loader": "*",
    "css-loader": "*",
    "style-loader": "*",
    "sass-loader": "*",
    "file-loader": "*",
    "postcss-loader": "*",
    "extract-text-webpack-plugin": "*",
    "browser-sync-webpack-plugin": "*",
    "webpack-manifest-plugin": "*",
    "uglifyjs-webpack-plugin": "*",
