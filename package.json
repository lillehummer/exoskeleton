      "babel-polyfill": "*",
      "browser-sync": "^2.24.4",
      "clean-webpack-plugin": "*",
      "css-loader": "*",
      "css-mqpacker": "*",
      "eslint": "*",
      "eslint-config-standard": "*",
      "eslint-plugin-import": "*",
